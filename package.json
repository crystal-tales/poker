{
    "name": "poker",
    "description": "Poker Masters",
    "version": "1.0.0",
    "scripts": {
        "ng": "ng",
        "start": "node src/server/index.js",
        "dist-win32": "build --tasks win-x86 --mirror https://dl.nwjs.io/ .",
        "dist-win64": "build --tasks win-x64 --mirror https://dl.nwjs.io/ .",
        "dist-lin32": "build --tasks linux-x86 --mirror https://dl.nwjs.io/ .",
        "dist-lin64": "build --tasks linux-x64 --mirror https://dl.nwjs.io/ .",
        "dist.mac64": "build --tasks mac-x64 --mirror https://dl.nwjs.io/ .",
        "serve": "ng serve",
        "build": "ng build",
        "watch": "ng build --watch --configuration development",
        "test": "ng test"
    },
    "private": true,
    "type": "module",
    "main": "./src/server/index.js",
    "build": {
        "nwVersion": "lts",
        "output": "./pkg/",
        "excludes": [
            "./src/assets/players/*",
            "./angular",
            "./angular/**/*",
            "./.temp-img",
            "./.temp-img/*",
            ".editorconfig",
            ".gitignore",
            ".README.md",
            "tsconfig.app.json",
            "tsconfig.json",
            "tsconfig.spec.json"
        ]
    },
    "dependencies": {
        "@angular/animations": "^15.0.0",
        "@angular/cdk": "^15.0.1",
        "@angular/common": "^15.0.0",
        "@angular/compiler": "^15.0.0",
        "@angular/core": "^15.0.0",
        "@angular/forms": "^15.0.0",
        "@angular/material": "^15.0.1",
        "@angular/platform-browser": "^15.0.0",
        "@angular/platform-browser-dynamic": "^15.0.0",
        "@angular/router": "^15.0.0",
        "body-parser": "^1.20.1",
        "cors": "^2.8.5",
        "deuces.js": "^0.0.2",
        "express": "^4.18.2",
        "express-async-wrapper": "^0.1.0",
        "@faker-js/faker": "^7.6.0",
        "file-type": "^18.2.0",
        "helmet": "^6.0.1",
        "lodash": "^4.17.21",
        "multer": "^1.4.5-lts.1",
        "ngx-toastr": "^16.0.1",
        "rxjs": "~7.5.0",
        "sharp": "^0.31.2",
        "tslib": "^2.3.0",
        "zone.js": "~0.12.0"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^15.0.2",
        "@angular/cli": "~15.0.2",
        "@angular/compiler-cli": "^15.0.0",
        "@types/jasmine": "~4.3.0",
        "@types/lodash": "^4.14.194",
        "jasmine-core": "~4.5.0",
        "karma": "~6.4.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage": "~2.2.0",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "~2.0.0",
        "nwjs-builder-phoenix": "^1.15.0",
        "typescript": "~4.8.2"
    }
}
